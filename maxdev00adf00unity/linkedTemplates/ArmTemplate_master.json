{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "maxdev00adf00unity"
		},
		"ErgastAPI_F1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ergast.com/api/f1/2021/1/results.json"
		},
		"maxdev_unity_ext_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://maxdev0unity0extstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "maxdev00adf00unity_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ErgastAPI_F1_properties_typeProperties_url": {
						"value": "[parameters('ErgastAPI_F1_properties_typeProperties_url')]"
					},
					"maxdev_unity_ext_storage_properties_typeProperties_url": {
						"value": "[parameters('maxdev_unity_ext_storage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}